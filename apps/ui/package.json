{
  "name": "@steadysass/ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ../../.env -- next dev",
    "prebuild": "yarn generate",
    "build": "env-cmd -f ../../.env -- next build",
    "generate": "yarn generate:typed-graphql-builder",
    "generate:typed-graphql-builder": "npx typed-graphql-builder --schema ../graphql/src/generated/schema.graphql --output ./src/generated/typed-graphql-builder.ts --includeTypename=true --scalar DateTime=string --scalar Time=string",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.10.6",
    "@graphql-yoga/node": "^3.9.1",
    "@hookform/resolvers": "^3.10.0",
    "@steadysass/graphql": "*",
    "graphql-tag": "^2.12.6",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "ts-pattern": "^5.6.2",
    "urql": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
